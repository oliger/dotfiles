{
  "breadcrumbs.enabled": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.lineHeight": 24,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80],
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": ["source.addMissingImports"],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "window.autoDetectColorScheme": true,
  "workbench.activityBar.visible": false,
  "workbench.iconTheme": null,
  "workbench.colorTheme": "Monochrome Dark",
  "workbench.preferredLightColorTheme": "Monochrome Light",
  "workbench.preferredDarkColorTheme": "Monochrome Dark",
  "workbench.startupEditor": "none",
  "git.enableSmartCommit": true,
  "vim.leader": "<space>",
  "vim.showmodename": false,
  "vim.camelCaseMotion.enable": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["leader", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["leader", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["leader", "."],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["leader", "e"],
      "commands": ["editor.action.marker.next"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["leader", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["leader", "."],
      "commands": ["editor.action.quickFix"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "[markdown]": {
    "editor.quickSuggestions": true,
    "editor.suggest.showSnippets": false,
    "editor.wordBasedSuggestions": false,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.useLanguageServer": true
}
