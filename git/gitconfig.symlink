[user]
	name = Jimmy Oliger
	email = jimmyoliger@gmail.com
[core]
	editor = code --new-window --wait
	excludesfile = ~/.gitignore
	ignorecase = false
[push]
	default = simple
[credential]
	helper = osxkeychain
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main